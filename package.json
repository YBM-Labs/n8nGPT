{
  "name": "n8ngpt",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "build:extension": "turbo run build --filter=n8ngpt",
    "build:backend": "turbo run build --filter=n8ngpt-backend",
    "dev": "turbo run dev",
    "dev:extension": "turbo run dev --filter=n8ngpt",
    "dev:backend": "turbo run dev --filter=n8ngpt-backend",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "prisma:studio": "cd apps/n8n-backend && pnpm prisma studio",
    "start:backend": "cd apps/n8n-backend && pnpm start"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "turbo": "^2.5.6",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
